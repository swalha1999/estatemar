{
	"name": "@estatemar/schemas",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"scripts": {
		"build": "tsc",
		"build:watch": "tsc --watch",
		"check-types": "tsc --noEmit",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"lint": "biome check .",
		"clean": "rm -rf lib/"
	},
	"exports": {
		".": {
			"types": "./lib/index.d.ts",
			"default": "./lib/index.js"
		},
		"./auth": {
			"types": "./lib/auth/index.d.ts",
			"default": "./lib/auth/index.js"
		},
		"./files": {
			"types": "./lib/files/index.d.ts",
			"default": "./lib/files/index.js"
		},
		"./properties": {
			"types": "./lib/properties/index.d.ts",
			"default": "./lib/properties/index.js"
		},
		"./common": {
			"types": "./lib/common/index.d.ts",
			"default": "./lib/common/index.js"
		},
		"./organizations": {
			"types": "./lib/organizations/index.d.ts",
			"default": "./lib/organizations/index.js"
		}
	},
	"dependencies": {
		"@web-std/file": "^3.0.3",
		"zod": "^4.0.2"
	},
	"devDependencies": {
		"@types/node": "^22.0.0",
		"typescript": "^5.8.0"
	}
}
