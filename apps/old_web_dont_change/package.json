{
    "name": "estatemar",
    "version": "0.1.0",
    "private": true,
    "packageManager": "pnpm@9.12.2",
    "engines": {
        "node": ">=20.0.0"
    },
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "drizzle-kit push && next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --write .",
        "db:push": "drizzle-kit push",
        "drizzle:push": "drizzle-kit push:mysql",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:studio": "drizzle-kit studio --port 6969",
        "db:create": "docker run --name estatemar-db -e POSTGRES_PASSWORD=mysecretpassword -e POSTGRES_USER=root -e POSTGRES_DB=estatemar -p 5432:5432 -d postgres:15",
        "db:remove": "pnpm db:stop && docker rm -f estatemar-db",
        "db:restore": "docker exec -i estatemar-db bash -c 'gunzip -c /backup.sql.gz | pg_restore -1 --no-owner --clean --if-exists -U root -d estatemar'",
        "db:copy": "docker cp ./backup/backup.sql.gz estatemar-db:/ && pnpm db:restore",
        "db:backup": "docker exec -i estatemar-db bash -c 'pg_dump -U root -d estatemar -f /backup.sql'",
        "db:setup": "pnpm db:create && sleep 5 && pnpm db:copy && sleep 5 && pnpm db:push",
        "db:start": "docker start estatemar-db",
        "db:stop": "docker stop estatemar-db",
        "seed": "node --loader ts-node/esm scripts/seed.ts"
    },
